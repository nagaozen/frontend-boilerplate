{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX and ES6 compilation",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "prebuild": "rm -f dist/*.js dist/*.map",
    "build": "webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nagaozen/frontend-boilerplate.git"
  },
  "author": "Fabio Zendhi Nagao <nagaozen@hotmail.com> (https://github.com/nagaozen/)",
  "license": "MIT",
  "dependencies": {
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-react-devtools": "^4.2.15"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
